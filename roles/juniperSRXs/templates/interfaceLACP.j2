

set chassis aggregated-devices ethernet device-count {{ aggregatedDeviceCount }}

set interfaces ge-0/0/{{ firstIntIndex }} ether-options 802.3ad ae0
set interfaces ge-0/0/{{ secondIntIndex }} ether-options 802.3ad ae0

set interfaces ae0 aggregated-ether-options lacp {{ lacpMode | default('active') }}
set interfaces ae0 aggregated-ether-options lacp periodic fast

# Configure minimum links (defaults to 1 if not specified)
set interfaces ae0 aggregated-ether-options minimum-links {{ minimumLinks | default(1) }}

